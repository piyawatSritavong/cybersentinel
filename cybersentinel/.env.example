# CyberSentinel AI - Environment Configuration
# Copy this file to .env and fill in your values.

# API Authentication
APP_API_KEY=your_secure_api_key_here
SECRET_VAULT_KEY=your-32-char-encryption-key-here!

# Groq Cloud AI Keys
GROQ_API_KEY=your_groq_api_key_here
ANALYST_MODEL=llama-3.3-70b-versatile
REVIEWER_MODEL=llama-3.3-70b-versatile

# Database (Supabase / PostgreSQL)
DATABASE_URL=postgresql://postgres:password@host:5432/postgres

# Optional External API Keys
SPLUNK_URL=https://mock-splunk.local
SPLUNK_TOKEN=mock-token
JIRA_URL=https://mock-jira.local
JIRA_TOKEN=mock-token
VT_API_KEY=mock-vt-key

# Ticketing Plugin configuration
TICKET_SYSTEM_TYPE=webhook
TICKET_WEBHOOK_URL=https://mock-webhook.local/receive
TICKET_EXPORT_PATH=data/exports/
